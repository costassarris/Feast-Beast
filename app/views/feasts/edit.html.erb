<%= form_for @feast, html: {multipart: true} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :twitter %>
  <%= f.text_field :twitter_id %>
  <%= f.label :instagram %>
  <%= f.text_field :instagram_user %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.file_field :image %>
  <%= f.submit %>
<% end %>