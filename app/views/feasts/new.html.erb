<%= form_for @feast, html: {multipart: true} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :twitter %>
  <%= f.text_field :twitter_id %>
  <%= f.label :instagram %>
  <%= f.text_field :instagram_user %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :category %>
  <%= f.select :category, options_for_select(%w[Food Zombie])%>
  <%= f.label :icon %>
  <%= f.select :icon, options_for_select(%w[food volcano caution fire mountain airport zombie]) %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit %>
<% end %>